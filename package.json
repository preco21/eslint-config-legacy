{
  "name": "eslint-config",
  "version": "7.0.0",
  "description": "ESLint shareable config for @preco21",
  "author": "Plusb Preco <plusb21@gmail.com>",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all --parallel lint check-eslint-rules",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "check-eslint-rules": "npm-run-all --parallel check-eslint-rules:*",
    "check-eslint-rules:main": "eslint-find-rules --unused --deprecated index.js",
    "check-eslint-rules:react": "eslint-find-rules --unused --deprecated react.js",
    "check-eslint-rules:react-native": "eslint-find-rules --unused --deprecated react-native.js"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "homepage": "https://github.com/preco21/eslint-config-prev#readme",
  "repository": "preco21/eslint-config-prev",
  "bugs": {
    "url": "https://github.com/preco21/eslint-config-prev/issues"
  },
  "files": [
    "rules",
    "index.js",
    "react.js",
    "react-native.js"
  ],
  "keywords": [
    "code",
    "quality",
    "style",
    "guide",
    "lint",
    "linter",
    "eslint",
    "config",
    "preco",
    "¯\\_(ツ)_/¯"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.24.0"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "eslint-find-rules": "^3.6.1",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "eslint": "^7.29.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": "./index.js",
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "quote-props": "off"
    }
  }
}
