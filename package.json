{
  "name": "eslint-config",
  "version": "7.0.0",
  "description": "ESLint shareable config for @preco21",
  "author": "Plusb Preco <plusb21@gmail.com>",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all --parallel lint check-eslint-rules",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.js\" \"**/*.md\"",
    "format:check": "prettier --list-different \"**/*.js\" \"**/*.md\"",
    "check-eslint-rules": "npm-run-all --parallel check-eslint-rules:*",
    "check-eslint-rules:main": "eslint-find-rules --unused --deprecated index.js",
    "check-eslint-rules:react": "eslint-find-rules --unused --deprecated react.js",
    "check-eslint-rules:react-native": "eslint-find-rules --unused --deprecated react-native.js"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "homepage": "https://github.com/preco21/eslint-config-prev#readme",
  "repository": "preco21/eslint-config-prev",
  "bugs": {
    "url": "https://github.com/preco21/eslint-config-prev/issues"
  },
  "files": [
    "rules",
    "index.js",
    "react.js",
    "react-native.js"
  ],
  "keywords": [
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "styleguide",
    "eslint",
    "preco",
    "¯\\_(ツ)_/¯"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "babel-eslint": "^10.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-react": "^7.17.0"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-find-rules": "^3.4.0",
    "is-plain-obj": "^2.0.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1"
  },
  "peerDependencies": {
    "eslint": "^6.7.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": "./index.js",
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "quote-props": "off"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  }
}
