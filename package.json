{
  "name": "@preco21/eslint-config",
  "version": "7.0.0",
  "description": "ESLint shareable config for @preco21",
  "author": "Plusb Preco <plusb21@gmail.com>",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all --parallel lint check-eslint-rules",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "check-eslint-rules": "npm-run-all --parallel check-eslint-rules:*",
    "check-eslint-rules:main": "eslint-find-rules --unused --deprecated index.js",
    "check-eslint-rules:react": "eslint-find-rules --unused --deprecated react.js",
    "check-eslint-rules:react-native": "eslint-find-rules --unused --deprecated react-native.js"
  },
  "engines": {
    "node": ">=14"
  },
  "license": "MIT",
  "homepage": "https://github.com/preco21/eslint-config-prev#readme",
  "repository": "preco21/eslint-config-prev",
  "bugs": {
    "url": "https://github.com/preco21/eslint-config-prev/issues"
  },
  "files": [
    "rules/",
    "index.js",
    "react.js",
    "react-native.js"
  ],
  "keywords": [
    "code",
    "style",
    "guide",
    "eslint",
    "config",
    "preco21",
    "¯\\_(ツ)_/¯"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "confusing-browser-globals": "^1.0.10",
    "eslint": "^7.31.0",
    "eslint-find-rules": "^3.6.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "confusing-browser-globals": "^1.0.10",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "confusing-browser-globals": {
      "optional": true
    },
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "./rules/base"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "script"
    },
    "rules": {
      "quote-props": "off"
    }
  }
}
